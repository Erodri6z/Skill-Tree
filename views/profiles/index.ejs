<%- include('../partials/html-head') %>

<%- include('../partials/nav') %>
<main>
  <br>

  <ul id="profile-list">
    <% profiles.forEach(profile => { %>
    <li>
      <div class="profile-style">
        <a href="/profiles/<%= profile._id %>">
        <img
          class="profiles"
          src="<%= profile.avatar %>"
          alt="<%= profile.name %>'s' avatar"
          />
        </a>
        <h3> <%= profile.name %> </h3>
      </div>
    </li>
    <% }) %>
  </ul>
</main>



<%- include('../partials/footer') %>